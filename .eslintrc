{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": 0,
        "indent": ["error", "tab"],
        "comma-dangle": ["error", "never"],
        "max-len": ["error", { "code": 100 }],
        "no-tabs": ["error", { "allowIndentationTabs": true }],
        "object-curly-spacing": "off",
        "require-jsdoc":"off"
    },
    "extends": [
        "eslint:recommended", "google"
    ]
}